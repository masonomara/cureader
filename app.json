{
  "expo": {
    "name": "Cureader",
    "slug": "cureader",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.webp",
    "scheme": "cureader",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.webp",
      "resizeMode": "cover",
      "backgroundColor": "#FF3E39"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.masonomara.cureader"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.webp",
        "backgroundColor": "#FF3E39"
      },
      "package": "cureader.android"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.webp"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9e098709-617a-4eac-b57d-d2b1d3407f78"
      }
    },
    "owner": "masonomara"
  }
}
